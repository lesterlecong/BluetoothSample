             xmlns:infrastructure="clr-namespace:BluetoothSample.Infrastructure"
             x:Class="BluetoothSample.Views.TestPage"
             xmlns:viewmodels="clr-namespace:BluetoothSample.ViewModels"
             x:DataType="viewmodels:TestViewModel"
             Title="Tests">
    <ContentPage.BindingContext>
        <viewmodels:TestViewModel />
    </ContentPage.BindingContext>

    <ContentPage.Content>
        <CollectionView ItemsSource="{Binding Tests}">
            <CollectionView.ItemTemplate>
                <DataTemplate>


                </DataTemplate>
            </CollectionView.ItemTemplate>
        </CollectionView>
    </ContentPage.Content>
</infrastructure:BaseContentPage>